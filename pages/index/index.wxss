/**index.wxss**/
@import "../../app.wxss";
page{
  background: #f2f3f5;
}
/*内容*/
.indexContent{
  width:100%;
}
.flex-wrap{
  width:95%;
  background:#fff;
  margin:0% auto 2%;
  padding:15px 2.5%;
  border-bottom:1px solid #eee;
}
.flex-wrap>view{
  display: inline-block;
}

/*客服*/
.free-contactButton{
  background:none !important;
  padding:0px !important;
  height:60rpx;
  position: fixed;
  bottom:20rpx;
  right:50rpx;
  z-index: 100;
}
.free-contactButton::after{
  border: none;
}
.iconfont{
  line-height: 60rpx;
  font-size:60rpx;
  color: #48c23d;
  background: none !important;
}
.iconfont::after{
  border: none !important;
}